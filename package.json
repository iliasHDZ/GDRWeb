{
  "name": "gdrweb",
  "version": "0.1.0",
  "description": "Geometry Dash Level Rendering Engine for the browser",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc -p .",
    "buildntest": "npx tsc -p . && browserify test/test.js > test/bundle.js",
    "server": "http-server -p 8000 -c -1",
    "buildlibrary": "npx tsc -p ./tsconfig_webpack.json && npx webpack --config webpack.config.js"
  },
  "keywords": [
    "geometry",
    "dash",
    "rendering"
  ],
  "author": "IliasHDZ",
  "license": "MIT",
  "dependencies": {
    "browserify": "^17.0.0",
    "http-server": "^14.0.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
